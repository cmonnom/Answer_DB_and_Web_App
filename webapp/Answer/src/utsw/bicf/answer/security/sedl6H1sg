package utsw.bicf.answer.security;

public class ClarityAPIAuthentication {
	
	String username;
	String password;
	String url;
	String dataOutputDirPath;
	
	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public String getDataOutputDirPath() {
		return dataOutputDirPath;
	}

	public void setDataOutputDirPath(String dataOutputDirPath) {
		this.dataOutputDirPath = dataOutputDirPath;
	}

}
